/******************查找弹窗**********************/
#main {    
    /*height:100vh;*/
    width:400vw;
    background-color: #EEEEEE;      
   }
   .blur{-webkit-filter: blur(10px);}   
   #dialog {
    position: absolute;
    left: 15%;
    top: 10%;
    width: 70%;
    height: 70%;
    display: none;    
   } 
   #content1{
    background-color: #DFDFDF;
    border: 1px #fff solid;
    border-radius: 5px;    
    box-shadow: 2px 2px 2px 2px #8dab6f;
    height:20vh;
    padding: 3em;
   }
   #vx{
    margin: auto;
    background-color: #fff;
    width: 2px;
    height: 10%;
   }
   #close{
    margin: auto;
    width: 80px;
    height: 80px;
    -webkit-border-radius:60px;
    background-color:#fff;
    line-height: 80px;
    font-size: 30px;
    text-align: center;
    cursor: pointer;
   }