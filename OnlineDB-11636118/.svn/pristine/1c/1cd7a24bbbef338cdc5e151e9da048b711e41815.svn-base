/*----------------chart----------------*/

table.formdata{
    border:1px solid #5F6F7E;
    border-collapse:collapse;
    font-family:Arial;
    text-align: center;
    border-collapse: collapse;
}

.row1{
    position:absolute;
    top:0;
    left:0;
}

 tr{
    background:#EEEEEE;
    
}               
tr:nth-child(2n){
    background:#DFDFDF; 
}
 tr{
    background-color: expression((this.sectionRowIndex % 2 == 0) ? "#DFDFDF" : "#EEEEEE" );
}        

table.formdata caption{
    text-align:center;
    padding-bottom:6px;
}
table.formdata th{
    border:1px solid #5F6F7E;
    background-color:#E2E2E2;
    color:#000000;
    
    font-weight:bold;
    text-align:center;
    font-weight:normal;
    padding:2px 8px 2px 6px;
    margin:0px;
}
table.formdata td{
    margin:0px;
    padding:0px;
    border:1px solid #ABABAB;   /* 单元格边框 */
}
table.formdata td{
    width:6em;
    height: 2em;
}
.btn{
    border:1px solid #0083f2;
    font-family:Arial;
}

