        $("#unlogin").click(function() {
            appcan.locStorage.remove('user_id'); 
            appcan.locStorage.remove('user_name'); 
            appcan.locStorage.remove('user_email'); 
            appcan.window.open({
                name : 'login',
                data : 'login.html',
