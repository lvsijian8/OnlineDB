            // 树状菜单
            $(document).ready(function() {
                $(".l1").toggle(function() {

                    $(this).next(".slist").animate({
                        height : 'toggle',
                        opacity : 'toggle'
                    }, "slow");
                }, function() {

                    $(this).next(".slist").animate({
                        height : 'toggle',
                        opacity : 'toggle'
                    }, "slow");
                });

                $(".l2").toggle(function() {
                    $(this).next(".sslist").animate({
                        height : 'toggle',
                        opacity : 'toggle'
                    }, "slow");
                }, function() {
                    $(this).next(".sslist").animate({
                        height : 'toggle',
                        opacity : 'toggle'
                    }, "slow");
                });

                $(".close").toggle(function() {
                    $(".slist").animate({
                        height : 'toggle',
                        opacity : 'hide'
                    }, "fast");
                    $(".sslist").animate({
                        height : 'toggle',
                        opacity : 'hide'
                    }, "fast");
                }, function() {
                    $(".slist").animate({
                        height : 'toggle',
                        opacity : 'show'
                    }, "fast");
                    $(".sslist").animate({
                        height : 'toggle',
                        opacity : 'show'
                    }, "fast");
                });
            });
        </script>

